  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>500 Error | Blogger</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Font Awesome -->
  <link rel="stylesheet" href="<%= publicPath %>plugins/fontawesome-free/css/all.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="<%= publicPath %>css/adminlte.min.css">
